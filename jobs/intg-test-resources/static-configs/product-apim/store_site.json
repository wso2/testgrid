  },
  "mutualAuthConfiguration": {
    "enabled": "false"
  },
  "forum": {
    "topicsPerPage": "5",
    "repliesPerPage": "5",
    "resourceIdentifier": "common"
  },
  "reverseProxy": {
    "enabled": true,
    "host": "localhost",
